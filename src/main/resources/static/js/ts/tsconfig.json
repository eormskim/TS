{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES6",
    "lib": ["ES2020", "DOM"],
    "sourceMap": true,
    "outDir": "../",  // js 폴더로 출력
    "rootDir": "./",   // 현재 ts 폴더
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "include": [
    "./**/*.ts"
  ],
  "exclude": [
    "../*.js"
  ]
}